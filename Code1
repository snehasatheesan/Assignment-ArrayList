 //1. Write a Java program that creates an `ArrayList` of strings.
    // Add five different fruits to the list, then retrieve and print the third fruit in the list.

package arraylist;

import java.util.ArrayList;
import java.util.Arrays;

public class Code1 {
    public static void main(String[] args) {
        ArrayList<String> fruitsNameStrings=new ArrayList<>(Arrays.asList("Apple","Banana","grapes","pinapple","Watermelon"));
        System.out.println(fruitsNameStrings.get(2));
    }
}
